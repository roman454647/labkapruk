def division(a,b):
    if(a<0 or b<0):
        print("error")
    if(a%b==0):
        return True
    else:
        return False
print("enter 2 number")

a = int(input())
b = int(input())

if(division(a,b)==True):
    print("correct")
else:
    print("incorrect")

